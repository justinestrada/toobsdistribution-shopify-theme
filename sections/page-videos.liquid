<div class="page container">

  <div class="row mb-5">
    <div class="col">
      <div class="section-header text-center">
        <h1 class="text-uppercase mb-0" >{{ page.title }}</h1>
      </div>
    </div>
  </div>

	<div class="row mb-5">
		<div class="col">
			<h3 class="text-uppercase m-0" >{{ section.settings.youtube_title_1 }}</h3>
		</div>
	</div>

  <div class="row mb-5">
  	<div class="col">
  		<div class="embed-responsive embed-responsive-16by9 z-depth-1" >
	  		<iframe class="embed-responsive-item" src="{{ section.settings.youtube_iframe_src_1 }}" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
	  	</div>
  	</div>
  </div>

  <div class="row mb-5">
    <div class="col">
      {{ page.content }}
    </div>
  </div>

</div>

{% schema %}
{
  "name": "Page Videos",
  "settings": [
    {
      "type": "text",
      "id": "youtube_title_1",
      "label": "YouTube Title",
      "default": "Toobs Talk Episode #13 CannaPods CBD"
    },
    {
      "type": "text",
      "id": "youtube_iframe_src_1",
      "label": "YouTube iFrame SRC 1",
      "default": "https://www.youtube.com/embed/DdhmxwnC7mg"
    }
  ]
}
{% endschema %}