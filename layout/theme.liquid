<!doctype html>
<!--[if lt IE 7]><html class="no-js lt-ie9 lt-ie8 lt-ie7" lang="en"> <![endif]-->
<!--[if IE 7]><html class="no-js lt-ie9 lt-ie8" lang="en"> <![endif]-->
<!--[if IE 8]><html class="no-js lt-ie9" lang="en"> <![endif]-->
<!--[if IE 9 ]><html class="ie9 no-js"> <![endif]-->
<!--[if (gt IE 9)|!(IE)]><!--> <html class="no-js"> <!--<![endif]-->
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  
  {% if template == 'index' %}
    <title>{{ shop.name }} | {{ page_title }}</title>
  {% elsif template == '404' %}
    <title>Page Not Found | {{ shop.name }}</title>
  {% else %}
    <title>{{ page_title }} | {{ shop.name }}</title>
  {% endif %}

  {%- if settings.favicon != blank -%}
    <link rel="shortcut icon" href="{{ settings.favicon | img_url: '32x32' }}" type="image/png">
  {%- endif -%}

  {% if page_description %}
    <meta name="description" content="{{ page_description }}" />
  {% endif %}

  <link rel="canonical" href="{{ canonical_url }}" />
  <meta name="viewport" content="width=device-width" />

  {{ 'slick.min.css' | asset_url | stylesheet_tag }}
  {{ 'slick-theme.min.css' | asset_url | stylesheet_tag }}
  {{ 'bootstrap.min.css' | asset_url | stylesheet_tag }}
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
  {{ 'style.css' | asset_url | stylesheet_tag }}

  <style>
  {% assign site_header_menu_font = settings.site_header_menu_font %}
  {{ site_header_menu_font | font_face }}

  {% assign site_headings_font = settings.site_headings_font %}
  {{ site_headings_font | font_face }}

  {% assign site_body_font = settings.site_body_font %}
  {{ site_body_font | font_face }}
  
  body {
    font-family: {{ site_body_font.family }}, sans-serif;
    font-style: {{ site_body_font.style }};
    font-weight: {{ site_body_font.weight }};
    letter-spacing: 1.5px;
    font-size: 14px;
  }
  h1, h2, h3, h4, h5, h6, .h1, .h2, .h3, .h4, .h5, .h6 {
    font-family: {{ site_headings_font.family }}, sans-serif;
    font-style: {{ site_headings_font.style }};
    font-weight: {{ site_headings_font.weight }};
    letter-spacing: 1.5px;
  }
  .header-nav__menu-link,
  .header-nav ul.header-desktop-menu li .header-nav__menu-link {
    font-family: {{ site_header_menu_font.family }}, sans-serif;
    font-style: {{ site_header_menu_font.style }};
    font-weight: {{ site_header_menu_font.weight }};
  }
  .confetti {
    width: 100%;
    display: none;
    margin: 0 auto;
    border: 1px solid #ddd;
    user-select: none;
    position: fixed;
    z-index: 20000;
    top: 0;
    bottom: 0;
 }
  </style>
  
  {{ 'jquery-2.1.3.min.js' | asset_url | script_tag }}
  
  {%- if template.directory == 'customers' -%}
    <script src="{{ 'shopify_common.js' | shopify_asset_url }}" defer="defer"></script>
  {%- endif -%}

  {{ content_for_header }}
</head>
<body class="template-{{ template | split: '.' | first }} pb-5 pb-sm-0" >
  {% section 'header' %} 
  {% section 'search-form' %}
  {{ content_for_layout }}
  {% section 'offcanvas-left-sidebar-menu' %}
  {% section 'offcanvas-right-sidebar-cart' %}
  {% section 'trust' %}
  {% section 'footer' %}
  <canvas class="confetti" id="canvas"></canvas>
  <script src="https://cdn.rawgit.com/jackmoore/zoom/master/jquery.zoom.min.js"></script>
  {{ 'slick.min.js' | asset_url | script_tag }}
  {{ 'popper.min.js' | asset_url | script_tag }}
  {{ 'bootstrap.min.js' | asset_url | script_tag }}
  {{ 'confetti.js' | asset_url | script_tag }}
  {{ 'theme.js' | asset_url | script_tag }}
<!-- BEEKETINGSCRIPT CODE START -->{% include 'bk-tracking' %}<!-- BEEKETINGSCRIPT CODE END -->
<!-- Back in Stock helper snippet -->
{% render 'back-in-stock-helper' %}
<!-- End Back in Stock helper snippet -->
</body>
</html>
